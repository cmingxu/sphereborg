.panel.panel-info
  .panel-heading
    %h3.panel-title=@node.id
  .panel-body
    = render :partial => "widgets/node_shared_nav"

    %table.table
      %thead
    
      %tbody
        - @images.each do |image|
          %tr
            %td= image.id
            %td= image.info["Names"]
            %td= image.info["Image"]
            %td= image.info["State"]
            %td= image.info["Status"]
